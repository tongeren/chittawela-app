{
    "compilerOptions": {
      "target": "esnext",
      "moduleResolution": "node",
      "allowJs": true,
      "noEmit": true,
      "strict": false,
      "isolatedModules": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "resolveJsonModule": true,
      "jsx": "preserve",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "skipLibCheck": true
    },
    "exclude": [
      "node_modules", "helpers/fullscreen/"
    ],
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx", "helpers/fullscreen/toggleFullScreen.js", "pages/index.js"
    ]
}
  
